{
  "name": "jambagecom/tt-board",
  "type": "typo3-cms-extension",
  "description": "Simple threaded (tree) or list message board (forum).",
  "keywords": [
    "TYPO3",
    "extension",
    "forum",
    "message",
    "board"
  ],
  "support": {
    "issues": "https://github.com/franzholz/tt_board/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": ">=8.0.0",
    "ext-pdo": "*",
    "typo3/cms-core": "^12",
    "typo3/cms-extensionmanager": "^12",
    "typo3/cms-felogin": "^^12",
    "typo3/cms-filemetadata": "^12",
    "typo3/cms-tstemplate": "^12",
    "typo3/minimal": "^12",
    "jambagecom/div2007": "^2.2",
    "jambagecom/tslib-fetce": "^0.9.0"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\TtBoard\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/tt-board": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "tt_board"
    }
  }
}
